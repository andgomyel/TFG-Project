<launch>
    <param name="/use_sim_time" value="true" />
    
    <!-- start world -->
    <node name="gazebo" pkg="gazebo_ros" type="gazebo"
          args="$(find turtlebot3_gazebo)/worlds/turtlebot3_house.world" respawn="false" output="screen" />

    <!-- include our robots -->
    <include file="$(find tfg)/launch/robots.launch"/>
    
    <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find multirobot_p3)/launch/config.rviz"/> -->
</launch>
